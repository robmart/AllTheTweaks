<Defs>
  <ThingDef ParentName="PlantFoodRawBase">
    <defName>RawCremini</defName>
    <label>cremini</label>
    <description>Raw Cremini mushrooms. Can be used as a substitute for meat.</description>
    <graphicData>
      <texPath>Things/Item/Resource/PlantFoodRaw/Cremini</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <socialPropernessMatters>true</socialPropernessMatters>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <DeteriorationRate>10</DeteriorationRate>
      <Mass>0.03</Mass>
      <Nutrition>0.05</Nutrition>
      <FoodPoisonChanceFixedHuman>0</FoodPoisonChanceFixedHuman>
      <MarketValue>4</MarketValue>
    </statBases>
    <ingestible>
      <preferability>RawTasty</preferability>
      <tasteThought></tasteThought>
      <foodType>VegetableOrFruit</foodType>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>RawVegetable_Eat</ingestSound>
    </ingestible>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>90</daysToRotStart>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
    <tickerType>Rare</tickerType>
    <thingCategories>
      <li>MeatRaw</li>
    </thingCategories>
  </ThingDef>
  
